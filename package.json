{
  "name": "projeto-modulo21",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node-dev src/server.ts",
    "build": "tsc",
    "start": "npm run migration:run && npm run seed && node dist/src/server.js",
    "migration:run": "prisma migrate deploy",
    "seed": "prisma db seed",
    "test:load-envs": "dotenv -e .env.test",
    "test": "npm run test:load-envs -- jest -- --runInBand",
    "test:coverage": "npm run test -- --coverage",
    "test:integration": "npm run test:load-envs -- jest tests/integration -- --runInBand",
    "test:integration:coverage": "npm run test:integration -- --coverage",
    "test:integration:migration:run": "npm run test:load-envs prisma migrate deploy",
    "test:unit": "jest tests/unit",
    "test:unit:coverage": "npm run test:unit -- --coverage"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "description": "",
  "dependencies": {
    "@prisma/client": "^6.3.1",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "cryptr": "^6.3.0",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-async-errors": "^3.1.1",
    "http-status": "^2.1.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "tsx": "^4.19.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.8.0",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/cryptr": "^4.0.3",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.8",
    "@types/node": "^22.13.2",
    "@types/supertest": "^6.0.3",
    "dotenv-cli": "^8.0.0",
    "jest": "^29.7.0",
    "prisma": "^6.3.1",
    "supertest": "^7.1.1",
    "ts-jest": "^29.3.4",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.7.3"
  }
}
